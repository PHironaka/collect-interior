<ul class="social">

<li>
<a href="https://www.facebook.com/CollectInterior/" target="_blank">Facebook</a>
</li>

<li>
<a href="https://www.instagram.com/collectinterior/" target="_blank">Instagram</a>
</li>

<li>
<a href="http://astronautmonastery.com" target="_blank">Pinterest</a>
</li>

</ul>
<footer>

  {%- assign footer_linklist = section.settings.footer_linklist -%}
  {% if linklists[footer_linklist].links.size > 0 %}
    <ul>
      <li>COPYRIGHT INTERIOR {{ 'now' | date: "%Y" }}</li>
      {% for link in linklists[footer_linklist].links %}
        <li><a href="{{ link.url }}">{{ link.title }}</a></li>
      {% endfor %}
    </ul>
  {% endif %}

  <form action="https://thechinatownmarket.us14.list-manage.com/subscribe/post?u=7641b08f28511416c16beebae&amp;id=31712a1353" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
    <div id="mc_embed_signup_scroll">
  
  <input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="your email here" required>
    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
    
    <div class="submit-button"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
    </div>
</form>

</footer>

{% schema %}
  {
    "name": "Footer",
    "settings": [
      {
        "type": "link_list",
        "id": "footer_linklist",
        "label": "First menu",
        "default": "footer"
      },
      {
        "type": "checkbox",
        "id": "show_payment_icons",
        "label": "Show payment icons",
        "default": false
      }
    ]
  }

{% endschema %}
